{
	"version" : "2017-02-28",
    "operation": "UpdateItem",
    "key": {
        "id": $util.dynamodb.toDynamoDBJson($ctx.args.id)
    },
    "update": {
    	"expression" : "SET settings.privacyManners = :privacyManners, settings.privacyResume = :privacyResume",
        "expressionValues": {
            ":privacyManners":$util.dynamodb.toDynamoDBJson($ctx.args.privacyManners),
            ":privacyResume":$util.dynamodb.toDynamoDBJson($ctx.args.privacyResume)
        }
    }
}